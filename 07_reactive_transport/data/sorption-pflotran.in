CHEMISTRY
MAX_RESIDUAL_TOLERANCE 1.e-15
PRIMARY_SPECIES
  Tracer1
  Tracer2
/
GENERAL_REACTION
  REACTION Tracer1 <-> Tracer2
  FORWARD_RATE 0.00005 ! 1/s
/


SORPTION
  ISOTHERM_REACTIONS
    Tracer1
     TYPE LINEAR
     DISTRIBUTION_COEFFICIENT 100
     /
    Tracer2
     TYPE LINEAR
     DISTRIBUTION_COEFFICIENT 50
     /
   /
  /


LOG_FORMULATION
DATABASE ./hanford-pflotran.dat
END
CONSTRAINT upstreambc
  CONCENTRATIONS
    Tracer1 1.0  T
    Tracer2 1.0e-20  T
  /
END
CONSTRAINT initial
  CONCENTRATIONS
    Tracer1 1.0e-20  T
    Tracer2 1.0e-20  T
  /
END
