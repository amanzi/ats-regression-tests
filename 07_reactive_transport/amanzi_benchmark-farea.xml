      <ParameterList name="cation_exchange_capacity">
        <Parameter name="value" type="double" value="2.75000000000000000e+00"/>
      </ParameterList>

      <ParameterList name="surface_site_density">
        <Parameter name="value" type="double" value="1.56199000000000005e-01"/>
      </ParameterList>

      <ParameterList name="atmospheric_pressure" type="ParameterList">
        <Parameter name="value" type="double" value="101325" />
