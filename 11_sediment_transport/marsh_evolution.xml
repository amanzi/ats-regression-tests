                <Parameter name="settling velocity" type="double" value="0.001" />
                <Parameter name="drag coefficient" type="double" value="0.005" />
                <!-- <Parameter name="settling velocity" type="double" value="0.0" /> -->
                <Parameter name="specific weight of water" type="double" value="9806." />
                <Parameter name="sediment density [kg m^-3]" type="double" value="2650" />
                <Parameter name="max current" type="double" value="0.2" />
                <Parameter name="Chezy parameter" type="double" value="10." />                
            </ParameterList>

            <ParameterList name="surface-erosion_rate" type="ParameterList">
                <Parameter name="field evaluator type" type="string" value="erosion rate" />
