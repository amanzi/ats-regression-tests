        <Parameter name="file" type="string" value="../mesh/swi_lab_test.exo" />
        <Parameter name="format" type="string" value="Exodus II" />
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="regions">
    <ParameterList name="computational domain">
      <ParameterList name="region: all" type="ParameterList" >
      </ParameterList>
    </ParameterList>
    <ParameterList name="east face">
      <ParameterList name="region: plane" type="ParameterList">
        <Parameter name="point" type="Array(double)" value="{3.4, 0., 0.0}"/>
        <Parameter name="normal" type="Array(double)" value="{1, 0, 0}"/>
