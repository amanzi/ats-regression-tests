<ParameterList name="Main" type="ParameterList">
  <ParameterList name="mesh" type="ParameterList">

    <ParameterList name="surface" type="ParameterList">
      <Parameter name="mesh type" type="string" value="surface" />
      <ParameterList name="surface parameters" type="ParameterList">
        <Parameter name="surface sideset name" type="string" value="top" />
      </ParameterList>
      <ParameterList name="surface">
      </ParameterList>
    </ParameterList>

    <ParameterList name="network" type="ParameterList">
      <Parameter name="mesh type" type="string" value="surface" />
      <ParameterList name="surface parameters" type="ParameterList">
        <Parameter name="surface sideset name" type="string" value="network" />
      </ParameterList>
      <ParameterList name="network">
      </ParameterList>
    </ParameterList>
    
    <ParameterList name="domain" type="ParameterList">
      <Parameter name="mesh type" type="string" value="read mesh file" />
      <ParameterList name="read mesh file parameters" type="ParameterList">
        <Parameter name="file" type="string" value="../data/pipe_network_1m.exo" />
        <Parameter name="format" type="string" value="Exodus II" />
        <ParameterList name="unstructured" type="ParameterList">
          <ParameterList name="expert" type="ParameterList">
            <Parameter name="partitioner" type="string" value="zoltan_rcb" />
          </ParameterList>
        </ParameterList>
      </ParameterList>
    </ParameterList>
    
  </ParameterList>

  <!-- REGIONS -->
  <ParameterList name="regions">
    <ParameterList name="computational domain">
      <ParameterList name="region: all" type="ParameterList">
      </ParameterList>
    </ParameterList>
    <ParameterList name="top" type="ParameterList">
      <ParameterList name="region: labeled set" type="ParameterList">
        <Parameter name="label" type="string" value="2" />
        <Parameter name="file" type="string" value="../data/pipe_network_1m.exo" />
        <Parameter name="format" type="string" value="Exodus II" />
        <Parameter name="entity" type="string" value="face" />
      </ParameterList>
    </ParameterList>

    <ParameterList name="surface" type="ParameterList">
      <ParameterList name="region: box" type="ParameterList">
        <Parameter name="low coordinate" type="Array(double)" value="{-1e+20,-1e+20}" />
        <Parameter name="high coordinate" type="Array(double)" value="{1e+20,1e+20}" />
      </ParameterList>
    </ParameterList>

    <ParameterList name="network domain" type="ParameterList">
      <ParameterList name="region: box" type="ParameterList">
        <Parameter name="low coordinate" type="Array(double)" value="{-1e+20,-1e+20}" />
        <Parameter name="high coordinate" type="Array(double)" value="{1e+20,1e+20}" />
      </ParameterList>
    </ParameterList>
    
    <ParameterList name="pipe1" type="ParameterList">
      <ParameterList name="region: box" type="ParameterList">
        <Parameter name="low coordinate" type="Array(double)"  value="{0.0, 14, -10.0}" />
        <Parameter name="high coordinate" type="Array(double)" value="{100, 15, 10.0}" />
      </ParameterList>
    </ParameterList>

    <ParameterList name="pipe2" type="ParameterList">
      <ParameterList name="region: box" type="ParameterList">
        <Parameter name="low coordinate" type="Array(double)"  value="{40, 0, -10.0}" />
        <Parameter name="high coordinate" type="Array(double)" value="{41, 29, 10.0}" />
      </ParameterList>
    </ParameterList>

    <!-- <ParameterList name="pipe1" type="ParameterList"> -->
    <!--   <ParameterList name="region: box" type="ParameterList"> -->
    <!--     <Parameter name="low coordinate" type="Array(double)"  value="{0.0, 4.2}" /> -->
    <!--     <Parameter name="high coordinate" type="Array(double)" value="{100, 4.5}" /> -->
    <!--   </ParameterList> -->
    <!-- </ParameterList> -->

    <!-- <ParameterList name="pipe2" type="ParameterList"> -->
    <!--   <ParameterList name="region: box" type="ParameterList"> -->
    <!--     <Parameter name="low coordinate" type="Array(double)"  value="{40, 0}" /> -->
    <!--     <Parameter name="high coordinate" type="Array(double)" value="{42, 10}" /> -->
    <!--   </ParameterList> -->
    <!-- </ParameterList> -->

    <ParameterList name="pipes" type="ParameterList">
      <ParameterList name="region: logical" type="ParameterList">
        <Parameter name="operation" type="string" value="union" />
        <Parameter name="regions" type="Array(string)" value="{pipe1, pipe2}" />
      </ParameterList>
    </ParameterList>
    
    <ParameterList name="network" type="ParameterList">
      <ParameterList name="region: logical" type="ParameterList">
        <Parameter name="operation" type="string" value="intersect" />
        <Parameter name="regions" type="Array(string)" value="{top, pipes}" />
      </ParameterList>
    </ParameterList>
    
    <ParameterList name="Inlet1">
      <ParameterList name="region: plane">
        <Parameter name="point" type="Array(double)" value="{0.0,14.5}"/>
        <Parameter name="normal" type="Array(double)" value="{-1.0,0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Inlet2">
      <ParameterList name="region: plane">
        <Parameter name="point" type="Array(double)" value="{0.0, 0.0}"/>
        <Parameter name="normal" type="Array(double)" value="{0.0,-1.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Inlet3">
      <ParameterList name="region: plane">
        <Parameter name="point" type="Array(double)" value="{0.0, 29}"/>
        <Parameter name="normal" type="Array(double)" value="{0.0, 1.0}"/>
      </ParameterList>
    </ParameterList>

    <ParameterList name="Inlet" type="ParameterList">
      <ParameterList name="region: logical" type="ParameterList">
        <Parameter name="operation" type="string" value="union" />
        <Parameter name="regions" type="Array(string)" value="{Inlet1, Inlet2, Inlet3}" />
      </ParameterList>
    </ParameterList>
    
    <ParameterList name="Outlet">
      <ParameterList name="region: plane">
        <Parameter name="point" type="Array(double)" value="{100.0,14.4}"/>
        <Parameter name="normal" type="Array(double)" value="{1.0, 0.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Manhole">
      <ParameterList name="region: point" type="ParameterList">
        <Parameter name="coordinate" type="Array(double)" value="{10, 4.4}"/>
      </ParameterList>
    </ParameterList>
        <ParameterList name="Junction">
      <ParameterList name="region: point" type="ParameterList">
        <Parameter name="coordinate" type="Array(double)" value="{40.5, 14.5}"/>
      </ParameterList>
    </ParameterList>
   
  </ParameterList>

  <ParameterList name="cycle driver" type="ParameterList">
    <Parameter name="start time" type="double" value=" 0" />
    <Parameter name="start time units" type="string" value="s" />
    <Parameter name="end time" type="double" value="100.0" />
    <Parameter name="end time units" type="string" value="s" />
   <!-- <Parameter name="end cycle" type="int" value="1000" /> -->
    <ParameterList name="PK tree" type="ParameterList">
      <ParameterList name="overland flow" type="ParameterList">
        <Parameter name="PK type" type="string" value="pipe flow" />
      </ParameterList>
    </ParameterList>
    <ParameterList name="verbose object">
      <Parameter name="verbosity level" type="string" value="medium"/>
    </ParameterList>
  </ParameterList>
  
  <ParameterList name="PKs" type="ParameterList">

    <ParameterList name="overland flow">
