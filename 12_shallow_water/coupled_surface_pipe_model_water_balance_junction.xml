<ParameterList name="Main" type="ParameterList">
  <ParameterList name="mesh" type="ParameterList">
    <ParameterList name="surface" type="ParameterList">
      <Parameter name="mesh type" type="string" value="aliased" />
      <ParameterList name="aliased parameters" type="ParameterList">
        <Parameter name="target" type="string" value="domain" />
      </ParameterList>
      <ParameterList name="surface">
      </ParameterList>
    </ParameterList>
    <ParameterList name="pipe" type="ParameterList">
      <Parameter name="mesh type" type="string" value="aliased" />
      <ParameterList name="aliased parameters" type="ParameterList">
        <Parameter name="target" type="string" value="domain" />
      </ParameterList>
      <ParameterList name="pipe">
      </ParameterList>
    </ParameterList>
    <ParameterList name="domain" type="ParameterList">
      <Parameter name="mesh type" type="string" value="generate mesh" />
      <ParameterList name="generate mesh parameters" type="ParameterList">
        <Parameter name="number of cells" type="Array(int)" value="{50, 1}" />
        <Parameter name="domain low coordinate" type="Array(double)" value="{0.0, 0.0 }" />
        <Parameter name="domain high coordinate" type="Array(double)" value="{200.0, 1.0}" />
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <!-- REGIONS -->
  <ParameterList name="regions">
    <ParameterList name="Computational domain">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0}"/>
        <Parameter name="high coordinate" type="Array(double)" value="{200.0,1.0}"/>
      </ParameterList>
    </ParameterList>
        <ParameterList name="Left plus junction">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{0.0,0.0}"/>
        <Parameter name="high coordinate" type="Array(double)" value="{72.0,1.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Right after junction">
      <ParameterList name="region: box">
        <Parameter name="low coordinate" type="Array(double)" value="{72.0,0.0}"/>
        <Parameter name="high coordinate" type="Array(double)" value="{200.0,1.0}"/>
      </ParameterList>
    </ParameterList>
    <ParameterList name="Inlet">
      <ParameterList name="region: plane">
        <Parameter name="point" type="Array(double)" value="{0.0,0.0}"/>
        <Parameter name="normal" type="Array(double)" value="{-1.0,0.0}"/>
      </ParameterList>
    </ParameterList>
        <ParameterList name="Outlet">
      <ParameterList name="region: plane">
        <Parameter name="point" type="Array(double)" value="{200.0,0.0}"/>
        <Parameter name="normal" type="Array(double)" value="{1.0,0.0}"/>
      </ParameterList>
    </ParameterList>
        <ParameterList name="Manhole">
      <ParameterList name="region: point" type="ParameterList">
    <Parameter name="coordinate" type="Array(double)" value="{100, 0.5}"/>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  <ParameterList name="cycle driver" type="ParameterList">
    <Parameter name="start time" type="double" value=" 0" />
    <Parameter name="start time units" type="string" value="s" />
    <Parameter name="end time" type="double" value="1200.0" />
    <Parameter name="end time units" type="string" value="s" />
    <Parameter name="end cycle" type="int" value="4001" />
    <ParameterList name="PK tree" type="ParameterList">
      <ParameterList name="coupled model" type="ParameterList">
        <Parameter name="PK type" type="string" value="weak MPC" />
        <ParameterList name="overland flow" type="ParameterList">
          <Parameter name="PK type" type="string" value="shallow water" />
        </ParameterList>
        <ParameterList name="pipe flow" type="ParameterList">
          <Parameter name="PK type" type="string" value="pipe flow" />
        </ParameterList>
      </ParameterList>
    </ParameterList>
    <ParameterList name="verbose object">
      <Parameter name="verbosity level" type="string" value="medium"/>
    </ParameterList>
  </ParameterList>
  
  <ParameterList name="PKs" type="ParameterList">

    <ParameterList name="coupled model" type="ParameterList">
      <Parameter name="PK type" type="string" value="weak MPC" />
      <Parameter name="PK name" type="string" value="coupled model" />
      <Parameter name="PKs order" type="Array(string)" value="{overland flow, pipe flow}" />
    </ParameterList>

    <!--********************** OVERLAND FLOW ********************* -->
    
    <ParameterList name="overland flow">
    <Parameter name="PK type" type="string" value="shallow water" /> 

      <Parameter name="domain name" type="string" value="surface"/>

      <ParameterList name="verbose object">
        <Parameter name="verbosity level" type="string" value="medium"/>
      </ParameterList>

      <Parameter name="source key" type="string" value="surface-pipe_drain"/>

      <ParameterList name="reconstruction">
        <Parameter name="polynomial order" type="int" value="0"/>
        <Parameter name="limiter" type="string" value="Barth-Jespersen"/>
        <Parameter name="limiter stencil" type="string" value="cell to closest cells"/>
        <Parameter name="limiter location" type="string" value="node"/>
        <Parameter name="limiter cfl" type="double" value="0.5"/>
      </ParameterList>
      
      <Parameter name="temporal discretization order" type="int" value="1"/>
      <Parameter name="cfl" type="double" value="2.5"/>
      <Parameter name="number of reduced cfl cycles" type="int" value="10"/>
      <Parameter name="numerical flux" type="string" value="Rusanov"/>

      <ParameterList name="boundary conditions">
        <ParameterList name="discharge">
          <ParameterList name="BC 0">
            <Parameter name="regions" type="Array(string)" value="{Inlet}"/>
            <Parameter name="spatial distribution method" type="string" value="none"/>
            <ParameterList name="discharge">
              <Parameter name="number of dofs" type="int" value="2"/>
              <Parameter name="function type" type="string" value="composite function"/>
              <ParameterList name="dof 1 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.005"/>
                </ParameterList>
              </ParameterList>
              <ParameterList name="dof 2 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.0"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
          <ParameterList name="BC 1">
            <Parameter name="regions" type="Array(string)" value="{Outlet}"/>
            <Parameter name="spatial distribution method" type="string" value="none"/>
            <ParameterList name="discharge">
              <Parameter name="number of dofs" type="int" value="2"/>
              <Parameter name="function type" type="string" value="composite function"/>
              <ParameterList name="dof 1 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.0001"/>
                </ParameterList>
              </ParameterList>
              <ParameterList name="dof 2 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.0"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
      </ParameterList>
      </ParameterList>

      <ParameterList name="source terms">
        <ParameterList name="surface-pipe_drain" type="ParameterList">
          <Parameter name="regions" type="Array(string)" value="{Manhole}" />
          <Parameter name="spatial distribution method" type="string" value="field" />
             <ParameterList name="source">
              <Parameter name="field key" type="string" value="surface-pipe_drain" />
             </ParameterList>
        </ParameterList>
      </ParameterList> 

    </ParameterList>

    <!--********************** PIPE FLOW PK********************* -->
    
    <ParameterList name="pipe flow">

      <Parameter name="domain name" type="string" value="pipe"/>

      <ParameterList name="verbose object">
        <Parameter name="verbosity level" type="string" value="extreme"/>
      </ParameterList>

      <Parameter name="source key" type="string" value="pipe-source_drain"/>
      
      <Parameter name="celerity" type="double" value="20"/>

      <Parameter name="direction key" type="string" value="pipe-direction"/>

      <Parameter name="diameter key" type="string" value="pipe-diameter"/>

      <Parameter name="Manning coefficient" type="double" value="0.009"/>

      <ParameterList name="reconstruction">
        <Parameter name="polynomial order" type="int" value="0"/>
        <Parameter name="limiter" type="string" value="Barth-Jespersen"/>
        <Parameter name="limiter stencil" type="string" value="cell to closest cells"/>
        <Parameter name="limiter location" type="string" value="node"/>
        <Parameter name="limiter cfl" type="double" value="1.0"/>
      </ParameterList>
      
      <Parameter name="temporal discretization order" type="int" value="1"/>
      <Parameter name="cfl" type="double" value="0.5"/>
      <Parameter name="number of reduced cfl cycles" type="int" value="10"/>
      <Parameter name="numerical flux" type="string" value="Rusanov"/>

      <ParameterList name="boundary conditions">
        <ParameterList name="discharge">
          <ParameterList name="BC 0">
            <Parameter name="regions" type="Array(string)" value="{Inlet}"/>
            <Parameter name="spatial distribution method" type="string" value="none"/>
            <ParameterList name="discharge">
              <Parameter name="number of dofs" type="int" value="2"/>
              <Parameter name="function type" type="string" value="composite function"/>
              <ParameterList name="dof 1 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.0001"/>
                </ParameterList>
              </ParameterList>
              <ParameterList name="dof 2 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.0"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
           <ParameterList name="BC 1">
            <Parameter name="regions" type="Array(string)" value="{Outlet}"/>
            <Parameter name="spatial distribution method" type="string" value="none"/>
            <ParameterList name="discharge">
              <Parameter name="number of dofs" type="int" value="2"/>
              <Parameter name="function type" type="string" value="composite function"/>
              <ParameterList name="dof 1 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.00001"/>
                </ParameterList>
              </ParameterList>
              <ParameterList name="dof 2 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.0"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="source terms">
        <ParameterList name="pipe-manhole_locations" type="ParameterList">
          <Parameter name="regions" type="Array(string)" value="{Manhole}" />
          <Parameter name="spatial distribution method" type="string" value="field" />
          <ParameterList name="source">
            <Parameter name="field key" type="string" value="pipe-source_drain" />
          </ParameterList>          
        </ParameterList>
      </ParameterList>

    </ParameterList>

    
  </ParameterList>

  <ParameterList name="state" type="ParameterList">

    <ParameterList name="evaluators">

      <ParameterList name="pipe-direction">
        <Parameter name="evaluator type" type="string" value="independent variable from file" />
        <Parameter name="filename" type="string" value="../data/pipe_direction_water_balance.h5" />
        <Parameter name="domain name" type="string" value="domain" />
        <Parameter name="variable name" type="string" value="pipe-direction" />
        <Parameter name="component name" type="string" value="cell" />
        <Parameter name="constant in time" type="bool" value="true" />
        <Parameter name="mesh entity" type="string" value="cell" />
        <Parameter name="number of dofs" type="int" value="2" />
      </ParameterList>

     <ParameterList name="pipe-diameter" type="ParameterList">
        <Parameter name="evaluator type" type="string" value="independent variable" />
        <ParameterList name="function">
          <ParameterList name="Left plus junction">
            <Parameter name="region" type="string" value="Left plus junction" />
            <Parameter name="component" type="string" value="cell" />
            <ParameterList name="function">
              <Parameter name="number of dofs" type="int" value="1" />
              <Parameter name="function type" type="string" value="composite function" />
              <ParameterList name="dof 1 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="1" />
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
          <ParameterList name="Right after junction">
            <Parameter name="region" type="string" value="Right after junction" />
            <Parameter name="component" type="string" value="cell" />
            <ParameterList name="function">
              <Parameter name="number of dofs" type="int" value="1" />
              <Parameter name="function type" type="string" value="composite function" />
              <ParameterList name="dof 1 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.5" />
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="surface-manhole_locations" type="ParameterList">
        <Parameter name="evaluator type" type="string" value="independent variable" />
        <ParameterList name="function">
          <ParameterList name="Computational domain">
            <Parameter name="region" type="string" value="Computational domain" />
            <Parameter name="component" type="string" value="cell" />
            <ParameterList name="function">
              <Parameter name="number of dofs" type="int" value="1" />
              <Parameter name="function type" type="string" value="composite function" />
              <ParameterList name="dof 1 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0" />
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
          <ParameterList name="Manhole">
            <Parameter name="region" type="string" value="Manhole" />
            <Parameter name="component" type="string" value="cell" />
            <ParameterList name="function">
              <Parameter name="number of dofs" type="int" value="1" />
              <Parameter name="function type" type="string" value="composite function" />
              <ParameterList name="dof 1 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="1" />
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="surface-pipe_drain" type="ParameterList">
        <Parameter name="evaluator type" type="string" value="pipe drain" />
        <Parameter name="manhole radius" type="double" value="0.12" />
        <Parameter name="energy losses coeff weir" type="double" value="0.1" />
        <Parameter name="energy losses coeff submerged weir" type="double" value="0.1" />
        <Parameter name="energy losses coeff orifice" type="double" value="0.1" />
        <Parameter name="pipe domain name" type="string" value="pipe" />
      </ParameterList>

      <ParameterList name="pipe-source_drain" type="ParameterList">
        <Parameter name="evaluator type" type="string" value="additive evaluator" />
        <Parameter name="dependencies" type="Array(string)" value="{surface-pipe_drain}" />
        <Parameter name="surface-pipe_drain coefficient" type="double" value="-1.0" />
      </ParameterList>

    </ParameterList>

    <ParameterList name="initial conditions">
      <ParameterList name="const_fluid_density">
        <Parameter name="value" type="double" value="1000.0"/>
      </ParameterList>

      <ParameterList name="atmospheric_pressure">
        <Parameter name="value" type="double" value="101325.0"/>
      </ParameterList>

      <ParameterList name="gravity">
        <Parameter name="value" type="Array(double)" value="{0.0, -9.81}"/>
      </ParameterList>

      <ParameterList name="surface-bathymetry">
        <ParameterList name="function">
          <ParameterList name="domain">
            <Parameter name="region" type="string" value="Computational domain"/>
            <Parameter name="components" type="Array(string)" value="{cell, node}"/>
            <ParameterList name="function">
              <ParameterList name="function-exprtk">
                <Parameter name="number of arguments" type="int" value="2"/>
                <Parameter name="formula" type="string" value="0.0"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="surface-total_depth">
        <ParameterList name="function">

          <ParameterList name="domain">
            <Parameter name="region" type="string" value="Computational domain"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <ParameterList name="function-exprtk">
                <Parameter name="number of arguments" type="int" value="2"/>
                <Parameter name="formula" type="string" value="0.01"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>

        </ParameterList>
      </ParameterList>


      <ParameterList name="surface-discharge">
        <ParameterList name="function">
          <ParameterList name="domain">
            <Parameter name="region" type="string" value="Computational domain"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <Parameter name="number of dofs" type="int" value="2"/>
              <Parameter name="function type" type="string" value="composite function"/>
              <ParameterList name="dof 1 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.0"/>
                </ParameterList>
              </ParameterList>
              <ParameterList name="dof 2 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.0"/>
                </ParameterList>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>


      <ParameterList name="pipe-bathymetry">
        <ParameterList name="function">
          <ParameterList name="domain">
            <Parameter name="region" type="string" value="Computational domain"/>
            <Parameter name="components" type="Array(string)" value="{cell, node}"/>
            <ParameterList name="function">
             <ParameterList name="function-exprtk">
                <Parameter name="number of arguments" type="int" value="2"/>
                <Parameter name="formula" type="string" value="0.0"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>

      <ParameterList name="pipe-total_depth">
        <ParameterList name="function">
          <ParameterList name="domain">
            <Parameter name="region" type="string" value="Computational domain"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <ParameterList name="function-exprtk">
                <Parameter name="number of arguments" type="int" value="2"/>
               <Parameter name="formula" type="string" value="0.01"/>
              </ParameterList>
            </ParameterList>
          </ParameterList>

        </ParameterList>
      </ParameterList>


      <ParameterList name="pipe-discharge">
        <ParameterList name="function">
          <ParameterList name="domain">
            <Parameter name="region" type="string" value="Computational domain"/>
            <Parameter name="component" type="string" value="cell"/>
            <ParameterList name="function">
              <Parameter name="number of dofs" type="int" value="2"/>
              <Parameter name="function type" type="string" value="composite function"/>
              <ParameterList name="dof 1 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.0"/>
                </ParameterList>
              </ParameterList>
              <ParameterList name="dof 2 function">
                <ParameterList name="function-constant">
                  <Parameter name="value" type="double" value="0.0"/>
                </ParameterList>
                </ParameterList>
            </ParameterList>
          </ParameterList>
        </ParameterList>
      </ParameterList>
      
    </ParameterList>
  </ParameterList>

  <ParameterList name="visualization">
    <!-- <ParameterList name="domain" type="ParameterList"> -->
    <!--   <Parameter name="times start period stop" type="Array(double)" value="{ 0, 1, -1}" />  -->
    <!--   <Parameter name="cycles start period stop" type="Array(int)" value="{ 0, 1, -1}" /> -->
    <!-- </ParameterList> -->
    <ParameterList name="surface" type="ParameterList">
     <Parameter name="times start period stop" type="Array(double)" value="{ 0, 1, -1}" />  
     <!--<Parameter name="cycles start period stop" type="Array(int)" value="{ 0, 50, -1}" />-->
    </ParameterList>
    <ParameterList name="pipe" type="ParameterList">
      <Parameter name="times start period stop" type="Array(double)" value="{ 0, 1, -1}" />  
     <!--<Parameter name="cycles start period stop" type="Array(int)" value="{ 0, 50, -1}" />-->
    </ParameterList>
  </ParameterList>

       <ParameterList name="observations" type="ParameterList">
    <ParameterList name="water balance" type="ParameterList">
      <Parameter name="observation output filename" type="string" value="coupled_water_balance.csv" />
      <Parameter name="times start period stop" type="Array(double)" value="{ 0, 1, -1}" />
      <Parameter name="times start period stop units" type="string" value="s" />
           <Parameter name="cycles start period stop" type="Array(int)" value="{ 0, 1, -1}" />
      <ParameterList name="observed quantities" type="ParameterList">
        <ParameterList name="swVolume" type="ParameterList">
          <Parameter name="variable" type="string" value="surface-ponded_depth" />
          <Parameter name="region" type="string" value="Computational domain" />
          <Parameter name="functional" type="string" value="integral" />
          <Parameter name="location name" type="string" value="cell" />
        </ParameterList>
        <ParameterList name="pipeVolume" type="ParameterList">
          <Parameter name="variable" type="string" value="pipe-wetted_area" />
          <Parameter name="region" type="string" value="Computational domain" />
          <Parameter name="functional" type="string" value="integral" />
          <Parameter name="location name" type="string" value="cell" />
        </ParameterList>
        <ParameterList name="swDrain" type="ParameterList">
          <Parameter name="variable" type="string" value="surface-pipe_drain" />
          <Parameter name="region" type="string" value="Manhole" />
          <Parameter name="functional" type="string" value="point" />
          <Parameter name="location name" type="string" value="cell" />
        </ParameterList>
        <ParameterList name="pipeDrain" type="ParameterList">
          <Parameter name="variable" type="string" value="pipe-source_drain" />
          <Parameter name="region" type="string" value="Manhole" />
          <Parameter name="functional" type="string" value="point" />
          <Parameter name="location name" type="string" value="cell" />
        </ParameterList>
      </ParameterList>
    </ParameterList>
  </ParameterList>

  
</ParameterList>
